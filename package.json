    "test": "turbo run test --env-mode=loose",
    "start": "turbo run start --env-mode=loose"
  },
  "dependencies": {
    "turbo": "2.4.4",
    "typescript": "catalog:types"
  },
  "packageManager": "pnpm@9.5.0"
}
