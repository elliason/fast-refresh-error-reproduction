{
  "name": "@mf/frontend-platform",
  "version": "0.1.0",
  "private": true,
  "description": "Set of tools for frontend web development for Media Factory CZ",
  "keywords": [
    "Media factory",
    "frontend",
    "platform",
    "react",
    "typescript",
    "storybook",
    "eslint",
    "prettier",
    "turbo"
  ],
  "author": "Media Factory CZ",
  "type": "module",
  "scripts": {
    "build": "turbo run build --env-mode=loose",
    "dev": "turbo run dev --env-mode=loose",
